ENV DOCKERFILE_ARG3 ${_ARG3:-Dockerfile not received}
ENV DOCKERFILE_ARG4 ${_ARG4:-Dockerfile not received}
ENV DOCKERFILE_ARG5 ${_ARG5:-Dockerfile not received}
ENV DOCKERFILE_ARG6 ${_ARG6:-Dockerfile not received}
ENV DOCKERFILE_ARG7 ${_ARG7:-Dockerfile not received}
ENV DOCKERFILE_ARG8 ${_ARG8:-Dockerfile not received}
ENV DOCKERFILE_ARG9 ${_ARG9:-Dockerfile not received}
